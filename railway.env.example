# إعدادات Railway/Render لتجاوز حظر IQ Option
# =============================================

# تفعيل Proxy لتجاوز حظر Railway/Render
USE_PROXY=true

# إعدادات IQ Option (اختيارية - يمكن تركها في الكود)
# IQ_EMAIL=your_email@gmail.com
# IQ_PASSWORD=your_password

# إعدادات الخادم
PORT=5000
NODE_ENV=production

# إعدادات إضافية للأداء
ENABLE_IQ_WS=false
MAX_RETRIES=3
CONNECTION_TIMEOUT=15

# إعدادات Proxy (متقدمة)
PROXY_ROTATION=true
PROXY_TEST_ON_STARTUP=true
PROXY_FALLBACK_TO_DIRECT=true

# إعدادات التحديث
UPDATE_INTERVAL=200
BATCH_SIZE=30
REQUEST_DELAY=10
